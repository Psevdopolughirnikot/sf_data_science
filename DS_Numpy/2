import numpy as np
arr = np.arange(8)
print(arr)
arr_new = arr.reshape((2,4), order='F')
print(arr_new)
arr.shape = (2,4)
print(arr)
arr_trans = arr.transpose()
print(arr_trans)

arr_1 = np.linspace(1, 2, 6)
print (arr_1[3])

nd_array = np.linspace(0, 6, 12, endpoint=False).reshape(3,4)
print(nd_array)
